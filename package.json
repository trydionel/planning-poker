{
  "name": "jeff-at-aha.scoring-poker",
  "description": "Community scoring (Kellymock)",
  "version": "0.0.0",
  "author": "trydionel",
  "repository": {
    "type": "git",
    "url": "https://github.com/trydionel/planning-poker/tree/scoring-poker"
  },
  "license": "MIT",
  "ahaExtension": {
    "cspSources": [],
    "contributes": {
      "settings": {
        "options": {
          "title": "Score options",
          "type": "number",
          "scope": ["account"],
          "array": true,
          "default": [0, 1, 2, 3, 4, 5, 6, 7]
        },
        "includeUnknown": {
          "title": "Include unknown option",
          "type": "boolean",
          "scope": ["account"],
          "default": false
        }
      },
      "views": {
        "scoringPoker": {
          "title": "Community scoring",
          "host": "attribute",
          "entryPoint": "src/views/planningPoker.tsx",
          "recordTypes": [
            "Ideas::Idea"
          ]
        }
      }
    }
  },
  "dependencies": {
    "@types/react": "*"
  },
  "devDependencies": {
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
